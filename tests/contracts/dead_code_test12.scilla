scilla_version 0

library Dead12

type A =
| A1
| A2
type B =
| B1 of A

let a2 = A2

contract Dead12()

procedure foo(b : B)
  match b with
  (* None of these are reported, because A is a part of B type, so, we
     assume that all of its constructors are reachable. *)
  | B1 (A1) =>
  | B1 (A2) =>
  end
end

transition bar()
  b = B1 a2;
  foo b
end
