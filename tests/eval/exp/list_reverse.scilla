let list_reverse =
  tfun 'A =>
  fun (l : List 'A) =>
    let folder = @list_foldl 'A (List 'A) in
    let init = Nil in
    let iter =
      fun (z : List 'A) =>
      fun (h : 'A) =>
        Cons {'A} h z
    in
      folder iter init l
in

let int_reverse = @list_reverse Int in

let one = 1 in
let two = 2 in
let ten = 10 in
let eleven = 11 in

let nil = Nil in
let l1 = Cons {Int} eleven nil in
let l2 = Cons {Int} ten l1 in
let l3 = Cons {Int} two l2 in
let l4 = Cons {Int} one l3 in

int_reverse l4
