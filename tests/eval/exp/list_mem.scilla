let list_mem =
  tfun 'A =>
  fun (f : 'A -> 'A -> Bool) =>
  fun (m : 'A) =>
  fun (l : List 'A) =>
    let folder = @list_foldl 'A Bool in
    let init = False in
    let iter =
      fun (z : Bool) =>
      fun (h : 'A) =>
        let res = f h m in
        match res with
        | True =>
          True
        | False =>
          z
        end
    in
      folder iter init l
in

let int_mem = @list_mem Int in
let f =
  fun (a : Int) =>
  fun (b : Int) =>
    builtin eq a b
in

let zero = 0 in
let one = 1 in
let two = 2 in
let three = 3 in
let four = 4 in
let five = 5 in

let nil = Nil in
let l1 = Cons {Int} four nil in
let l2 = Cons {Int} three l1 in
let l3 = Cons {Int} two l2 in
let l4 = Cons {Int} one l3 in

let res1 = int_mem f zero l4 in
let res2 = int_mem f one l4 in
let res3 = int_mem f five l4 in

int_mem f three l4
