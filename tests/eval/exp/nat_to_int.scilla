let nat_to_int =
  fun (n : Nat) =>
		let f =
      fun (z : Int32) =>
      fun (n : Nat) =>
        match n with
        | _ =>
          let one_int = Int32 1 in
          builtin add z one_int
        end
    in
	  let folder = @nat_fold Int32 in
    let zero_int = Int32 0 in
      folder f zero_int n
in

let zero = Zero in
let one = Succ zero in
let two = Succ one in
let three = Succ two in

