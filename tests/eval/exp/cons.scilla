(* Expected result *)
(* (ValLit(Msg((tag(StringLit Main))(value(IntLit 5))))) *)

let nil_msg = Nil {Message} in
let one_msg = 
  fun (msg : Message) => 
   Cons {Message} msg nil_msg
in
let msg = {tag: Main; value: 5} in
let msgs = one_msg msg in
match msgs with
| Cons m _ => m
| _ => {tag: Fail; value: 0}
end
