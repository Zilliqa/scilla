# To install the compatible version of `secp256k1` package, one needs to add the following tap:
tap "domt4/crypto"

brew "gcc"
brew "ocaml"
brew "opam"
brew "pkg-config"
brew "libffi"
brew "openssl"
brew "boost"
brew "pcre"
brew "domt4/crypto/secp256k1", args: ["with-enable-module-recovery"]
brew "cmake"
